{
  "name": "krai-tech",
  "version": "1.0.0",
  "description": "KRAIKIT Library is a set of components, services, directives and other tools to build UI using Angular framework",
  "keywords": [
    "angular",
    "kit",
    "ui",
    "component-library",
    "component",
    "service",
    "directive"
  ],
  "homepage": "https://github.com/krai-tech/krai-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krai-tech/krai-kit.git"
  },
  "maintainers": [
    "Eugene Rusakov <john.rusakov@gmail.com>"
  ],
  "workspaces": [
    "projects/*"
  ],
  "license": "MIT",
  "scripts": {
    "start-showcase": "npx nx serve showcase",
    "build-showcase": "npx nx build showcase",
    "build-cdk-kit-api": "npx nx run-many --target=build --projects=tag:cdk,tag:web-api,tag:kit --no-cloud",
    "build-cdk": "npx nx build cdk --prod --no-cloud",
    "build-kit": "npx nx build kit --prod --no-cloud",
    "build-web-api": "npx nx build @krai-web-api --prod --no-cloud",
    "test-cdk-kit-api": "npx nx run-many --target=test --projects=tag:cdk,tag:web-api,tag:kit --no-cloud",
    "test-cdk": "npx nx test cdk --no-cloud",
    "test-kit": "npx nx test kit --no-cloud",
    "test-web-api": "npx nx test @krai-web-api --no-cloud",
    "lint-cdk-kit-api": "npx nx run-many --target=lint --projects=tag:cdk,tag:web-api,tag:kit --no-cloud",
    "lint-cdk": "npx nx lint cdk --no-cloud",
    "lint-kit": "npx nx lint kit --no-cloud",
    "lint-web-api": "npx nx lint @krai-web-api --no-cloud",
    "release-cdk-kit-api": "npx nx run-many --target=release --projects=tag:cdk,tag:web-api,tag:kit --parallel=false --no-cloud",
    "release-cdk": "npx nx run cdk:release --no-cloud",
    "release-kit": "npx nx run kit:release --no-cloud",
    "release-web-api": "npx nx run @krai-web-api:release --no-cloud",
    "start testing": "nx serve testing",
    "prepare": "husky install"
  },
  "authors": [
    "Eugene Rusakov <john.rusakov@gmail.com>"
  ],
  "engines": {
    "node": ">= 18",
    "npm": ">= 8",
    "yarn": "Please use npm instead of yarn to install dependencies"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.3",
    "@angular/cdk": "18.2.3",
    "@angular/common": "18.2.3",
    "@angular/compiler": "18.2.3",
    "@angular/core": "18.2.3",
    "@angular/forms": "18.2.3",
    "@angular/platform-browser": "18.2.3",
    "@angular/platform-browser-dynamic": "18.2.3",
    "@angular/router": "18.2.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/config-nx-scopes": "^19.3.1",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@ng-doc/app": "17.6.0",
    "@ng-doc/builder": "17.6.0",
    "@ng-doc/core": "17.6.0",
    "@ng-doc/ui-kit": "17.6.0",
    "@nx/angular": "19.6.6",
    "@types/lodash": "^4.17.4",
    "commitizen": "^4.3.0",
    "flag-icons": "^7.2.3",
    "lodash": "^4.17.21",
    "rxjs": "~7.8.0",
    "semantic-release-npm-github-publish": "^1.5.5",
    "semantic-release-plus": "^20.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.3",
    "@angular-devkit/core": "18.2.3",
    "@angular-devkit/schematics": "18.2.3",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "~17.3.0",
    "@angular/compiler-cli": "18.2.3",
    "@angular/language-service": "18.2.3",
    "@nx/eslint": "19.6.6",
    "@nx/eslint-plugin": "19.6.6",
    "@nx/jest": "19.6.6",
    "@nx/js": "19.6.6",
    "@nx/workspace": "19.6.6",
    "@schematics/angular": "18.2.3",
    "@swc-node/register": "1.9.2",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.13",
    "@theunderscorer/nx-semantic-release": "^2.11.0",
    "@types/jest": "29.5.12",
    "@types/node": "^22.0.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "^10.4.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "14.1.1",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^15.2.5",
    "ng-packagr": "18.2.1",
    "nx": "19.6.6",
    "postcss": "^8.4.5",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "semantic-release": "^24.0.0",
    "semantic-release-plugin-decorators": "^4.0.0",
    "tailwindcss": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.5.4"
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "18.3.0",
    "@nx/nx-darwin-x64": "18.3.0",
    "@nx/nx-linux-x64-gnu": "18.3.0",
    "@nx/nx-win32-x64-msvc": "18.3.0"
  }
}
